<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="customer" class="solution1.Customer">
            <constructor-arg index="0" value="Raja"/>
            <constructor-arg index="1" value="123"/>
            <constructor-arg index="2" value="900898797"/>
            <constructor-arg index="3" ref="address"/>
        </bean>
        <bean id="address" class="solution1.Address">

            <constructor-arg index="0" value="rajendra nagar"/>
            <constructor-arg index="1" value="Uttar Pradesh"/>
            <constructor-arg index="2" value="Bareilly"/>
            <constructor-arg index="3" value="India"/>
            <constructor-arg index="4" value="200233"/>
        </bean>


</beans>